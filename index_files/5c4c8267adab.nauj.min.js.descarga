document.addEventListener('DOMContentLoaded',function(){const applyFiltersButtons=document.querySelectorAll('.apply-filters__button');if(applyFiltersButtons){applyFiltersButtons.forEach(function(button){button.addEventListener('click',function(){const popup=button.closest('.jet-popup');const bodyElement=document.body;if(popup){popup.classList.remove('jet-popup--show-state');popup.classList.add('jet-popup--hide-state');bodyElement.classList.remove('jet-popup-prevent-scroll')}})})}
const fixedHeader=document.getElementById('ast-fixed-header');var searchBox=document.querySelectorAll('.ast-below-header-wrap');window.addEventListener('scroll',function(){if(window.scrollY>=137){fixedHeader.style.visibility='visible';document.documentElement.classList.add('ast-header-stick-slide-active');document.body.classList.add('ast-primary-sticky-header-active','ast-above-sticky-header-active');if(document.body.classList.contains('home')){searchBox[1].style.visibility='hidden'}}else{fixedHeader.style.visibility='hidden';document.documentElement.classList.remove('ast-header-stick-slide-active');document.body.classList.remove('ast-primary-sticky-header-active','ast-above-sticky-header-active')}});const raq_label_NodeList=document.querySelectorAll('.raq_label');if(raq_label_NodeList){raq_label_NodeList.forEach(function(raq_label){raq_label.addEventListener('click',()=>{window.location=raq_label.getAttribute("href")})})}
const observer=new MutationObserver((mutations)=>{mutations.forEach((mutation)=>{if(mutation.type==='attributes'||mutation.type==='childList'||mutation.attributeName==="onchange"){forceFuckingClick()}})});raq_label_NodeList.forEach((elemento)=>{observer.observe(elemento,{attributes:!0,attributeFilter:["onchange"]})});function forceFuckingClick(){const raq_label=document.querySelector('.raq_label');raq_label.addEventListener('click',()=>{window.location=raq_label.getAttribute("href")})}})